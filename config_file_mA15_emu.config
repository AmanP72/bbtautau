
	% --------------- %	
	% ---  JOB    --- %
	% --------------- %

	Job: "analysis_mA15_emu"
        POI: "HiggAAbbtautauBr"
        Lumi: 1
        % --- on input ntuples ---
        ReadFrom: HIST
        HistoPath: "/afs/cern.ch/user/a/amaman/mA15_hist_combined"
        % --- condor compatibility ---
        SplitHistoFiles: TRUE
        % --- on systematics ---
        StatOnly: FALSE
        SystPruningShape: 0.0083
        SystPruningNorm: 0.0083
        CorrelationThreshold: 0.25
        SystErrorBars: TRUE
        RemoveSystOnEmptySample: TRUE
        SystControlPlots: FALSE
        SystDataPlots: FALSE
        MCstatThreshold: 0.01
        % --- on ranking plots ---
        RankingMaxNP: 20
        RankingPOIName: HiggAAbbtautauBr
        %RankingPlot: all
        % --- on run errors & warnings ---
        DebugLevel: 1
        HistoChecks: NOCRASH
        % --- plot cosmetics ---
        PlotOptions: OVERSIG %,NORATIO" %YIELD
        ImageFormat: "png"
        AtlasLabel: "Internal (20)"
        Label: "m_{a} = 20 GeV"
        CmeLabel: "13 TeV"
        LumiLabel: "140 fb^{-1}"
        % --- blinding SRs ---
        BlindSRs: FALSE
        KeepPrefitBlindedBins: FALSE
        RatioYmax: 2.0
        RatioYmin: 0.0
        RatioYmaxPostFit: 2.0
        RatioYminPostFit: 0.0
        UseFolderStructure: FALSE

    % --------------- %
    % ---  FIT    --- %
    % --------------- %

    Fit: "analysis_mA15_emu"
        FitType: SPLUSB
        FitRegion: CRSR
        FitBlind: FALSE
        SetRandomInitialNPval: 0.1
        POIAsimov: 0
        NumCPU: 4




